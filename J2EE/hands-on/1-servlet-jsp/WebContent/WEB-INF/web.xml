<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>first</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>Hello</display-name>
    <servlet-name>Hello</servlet-name>
    <servlet-class>com.training.servlet.Hello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SevletExample</display-name>
    <servlet-name>SevletExample</servlet-name>
    <servlet-class>com.training.servlet.SevletExample</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SevletExample</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GSevletExample</display-name>
    <servlet-name>GSevletExample</servlet-name>
    <servlet-class>com.training.servlet.GenericServletExample</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GSevletExample</servlet-name>
    <url-pattern>/gservlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>HttpSevletForm</display-name>
    <servlet-name>HttpSevletForm</servlet-name>
    <servlet-class>com.training.servlet.HelloForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HttpSevletForm</servlet-name>
    <url-pattern>/sayhello</url-pattern>
  </servlet-mapping>
</web-app>